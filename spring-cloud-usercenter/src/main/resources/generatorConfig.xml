<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!--导入属性配置 -->
    <!--<properties resources="generator.properties"></properties> -->

    <!--指定特定数据库的jdbc驱动jar包的位置 -->
    <classPathEntry
            location="D:\work\mavenrepo\m2\com\oracle\ojdbc-7\12.10.2\ojdbc-7-12.10.2.jar" />

    <context id="oracle" targetRuntime="MyBatis3">

        <plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin"></plugin>
        <plugin type="org.mybatis.generator.plugins.CachePlugin"></plugin>
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>
        <plugin type="org.mybatis.generator.plugins.MapperConfigPlugin"></plugin>
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin"></plugin>

        <plugin type="com.footprint.common.config.mybatis.OracleUpdatePlugin"></plugin>
        <plugin type="com.footprint.common.config.mybatis.OraclePaginationPlugin"></plugin>

        <!-- optional，旨在创建class时，对注释进行控制 -->
        <commentGenerator
                type="com.footprint.common.config.mybatis.MyBatisCommentGenerator">
            <property name="suppressDate" value="true" />
            <property name="suppressAllComments" value="false" />
        </commentGenerator>

        <!--jdbc的数据库连接 -->
        <jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
                        connectionURL="jdbc:oracle:thin:@192.168.1.105:1521:eCottonDB"
                        userId="cotton" password="cotton">
            <property name="remarksReporting" value="true"></property>
        </jdbcConnection>

        <!-- 非必需，类型处理器，在数据库类型和java类型之间的转换控制 -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!-- Model模型生成器,用来生成含有主键key的类，记录类 以及查询Example类 targetPackage 指定生成的model生成所在的包名
            targetProject 指定在该项目下所在的路径 -->
        <javaModelGenerator targetPackage="com.footprint.common.model"
                            targetProject="src/main/java">
            <!-- 是否对model添加 构造函数 -->
            <property name="constructorBased" value="true"/>
            <!-- 是否允许子包，即targetPackage.schemaName.tableName -->
            <property name="enableSubPackages" value="false" />

            <!-- 给Model添加一个父类 -->
            <!--<property name="rootClass" value="usercenter.upms.dao.model.BaseMode" />-->
            <!-- 是否对类CHAR类型的列的数据进行trim操作 -->
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!--Mapper映射文件生成所在的目录 为每一个数据库的表生成对应的SqlMap文件 -->
        <sqlMapGenerator targetPackage="mapper"
                         targetProject="src/main/resources">
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>

        <!-- 客户端代码，生成易于使用的针对Model对象和XML配置文件 的代码 type="ANNOTATEDMAPPER",生成Java Model
            和基于注解的Mapper对象 type="MIXEDMAPPER",生成基于注解的Java Model 和相应的Mapper对象 type="XMLMAPPER",生成SQLMap
            XML文件和独立的Mapper接口 -->
        <javaClientGenerator targetPackage="com.footprint.common.dao"
                             targetProject="src/main/java" type="XMLMAPPER">
            <property name="enableSubPackages" value="" />
            <!-- 定义Maper.java 源代码中的ByExample() 方法的可视性，可选的值有： public; private; protected;
                default 注意：如果 targetRuntime="MyBatis3",此参数被忽略 -->
            <!--<property name="exampleMethodVisibility" value=""/> -->
            <!-- 方法名计数器 Important note: this property is ignored if the target runtime
                is MyBatis3. -->
            <!--<property name="methodNameCalculator" value=""/> -->

            <property name="enableSubPackages" value="true" />
            <!-- 为生成的接口添加父接口 -->
            <!--<property name="rootInterface" value=""/> -->
        </javaClientGenerator>

        <table tableName="D_UC_ACCOUNT_TYPE_ROLE" />
        <table tableName="D_UC_BUSINESS_SYSTEM" />
        <table tableName="D_UC_DEALER" />
        <table tableName="D_UC_DEALER_AUDIT_INFO" />
        <table tableName="D_UC_ENTERPRISE" />
        <table tableName="D_UC_ENTERPRISE_AUDIT_INFO" />
        <table tableName="D_UC_ENTP_AUDIT" />
        <table tableName="D_UC_ENTP_AUDIT_HIS" />
        <table tableName="D_UC_ENTP_TYPE_MAP" />
        <table tableName="D_UC_FIN" />
        <table tableName="D_UC_FIN1" />
        <table tableName="D_UC_GOV" />
        <table tableName="D_UC_GREEN_SUPPLIER" />
        <table tableName="D_UC_ORG" />
        <table tableName="D_UC_ORG_A_BLACK" />
        <table tableName="D_UC_ORG_A_WHITE" />
        <table tableName="D_UC_ORG_BANK_ACCOUNT" />
        <table tableName="D_UC_ORG_PERSON" />
        <table tableName="D_UC_ORG_RELATION" />
        <table tableName="D_UC_ORG_ROLE" />
        <table tableName="D_UC_ORG_USER" />
        <table tableName="D_UC_PERMISSION" />
        <table tableName="D_UC_PERSON" />
        <table tableName="D_UC_ROLE" />
        <table tableName="D_UC_ROLE_PERMISSION" />
        <table tableName="D_UC_SYS_ROLE" />
        <table tableName="D_UC_TEXTILE_ENTP" />
        <table tableName="D_UC_USER" />
        <table tableName="D_UC_USER_BLACK" />
        <table tableName="D_UC_USER_OAUTH" />
        <table tableName="D_UC_USER_PERMISSION" />
        <table tableName="D_UC_USER_ROLE" />
        <table tableName="D_UC_WF" />

    </context>
</generatorConfiguration>