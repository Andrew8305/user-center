<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>16</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste Simple">
        <Item>
          <IdeaActionID>EditorPasteSimple</IdeaActionID>
          <ShortCut>'⌥⇧⌘V'</ShortCut>
          <Description>Paste Simple</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'⌥⌘R' or 'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'UCApplication'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧R'</ShortCut>
          <Description>Run 'UCApplication'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'UCApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'UCApplication'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'⌘9' or '⇧⌘9'</ShortCut>
          <Description>Version Control</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>